{
 "exe": "enclave",
 "key": "private.pem",
 "debug": false,
 "heapSize": 512,
 "executableHeap": false,
 "productID": 1,
 "securityVersion": 1,
 "mounts": [
     {
         "source": "mount",
         "target": "mount",
         "type": "hostfs",
         "readOnly": false
     }
 ],
 "env": [
        {
            "name": "COINGECKO_API_KEY",
            "fromHost": true
        },
        {
            "name": "COINGECKO_PRO_API_KEY",
            "fromHost": true
        }
 ],
 "files": [
     {
         "source": "cacert.pem",
         "target": "/etc/ssl/certs/ca-certificates.crt"
     }
 ]
}
